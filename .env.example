# Azure SQL Database Connection Configuration
# Copy this file to .env and fill in your actual credentials
# DO NOT commit .env to git!

AZURE_SQL_SERVER=tillsqlserver.database.windows.net
AZURE_SQL_DATABASE=TILLDBWEB_Prod
AZURE_SQL_USER=tillsqladmin
AZURE_SQL_PASSWORD=your_password_here
AZURE_SQL_PORT=1433

# Connection String (alternative)
# AZURE_SQL_CONNECTION_STRING=Server=tcp:tillsqlserver.database.windows.net,1433;Database=TILLDBWEB_Prod;User ID=tillsqladmin;Password=your_password;Encrypt=yes;TrustServerCertificate=no;Connection Timeout=30;
